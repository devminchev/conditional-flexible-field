{
  "name": "conditional-flexible-field",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@contentful/app-sdk": "^4.29.1",
    "@contentful/f36-components": "4.77.4",
    "@contentful/field-editor-dropdown": "^1.5.16",
    "@contentful/field-editor-reference": "^6.5.0",
    "@contentful/field-editor-shared": "^2.4.2",
    "@contentful/field-editor-single-line": "^1.5.3",
    "@contentful/react-apps-toolkit": "1.2.16",
    "emotion": "10.0.27",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "cross-env BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "eject": "react-scripts eject",
    "create-app-definition": "contentful-app-scripts create-app-definition",
    "upload": "contentful-app-scripts upload --bundle-dir ./build",
    "upload-ci": "contentful-app-scripts upload --ci --bundle-dir ./build --organization-id $CONTENTFUL_ORG_ID --definition-id $CONTENTFUL_APP_DEF_ID --token $CONTENTFUL_ACCESS_TOKEN",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@contentful/app-scripts": "1.32.2",
    "@contentful/field-editor-test-utils": "^1.5.2",
    "@testing-library/jest-dom": "5.17.0",
    "@testing-library/react": "14.3.1",
    "@tsconfig/create-react-app": "2.0.5",
    "@types/node": "^20.6.5",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.1",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "cross-env": "^7.0.3",
    "gh-pages": "^6.1.1",
    "ts-jest": "^29.3.4",
    "typescript": "^5.4.5"
  },
  "homepage": "."
}
